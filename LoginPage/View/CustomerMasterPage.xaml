<?xml version="1.0" encoding="UTF-8"?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="LoginPage.CutomerListPage" IsPresented="true">
	<MasterDetailPage.Master>
		<ContentPage Title="Customer" xmlns:ViewModel="clr-namespace:LoginPage">
			<ContentPage.BindingContext>
				<ViewModel:PrescriberViewModel>
				</ViewModel:PrescriberViewModel>
			</ContentPage.BindingContext>
			<ContentPage.Content>
				<StackLayout Orientation="Vertical" BackgroundColor="Silver">
					<ListView ItemsSource="{Binding ContactList}" ItemSelected="Handle_ItemSelected" HeightRequest="120" x:Name="contactCustomerList" HasUnevenRows="true" Margin="100,0,100,0" SeparatorColor="Transparent" IsVisible="true">
						<ListView.ItemTemplate>
							<DataTemplate>
								<ViewCell>
									<StackLayout Orientation="Vertical">
										<Label Text="{Binding fullname}">
										</Label>
									</StackLayout>
								</ViewCell>
							</DataTemplate>
						</ListView.ItemTemplate>
					</ListView>
				</StackLayout>
			</ContentPage.Content>
		</ContentPage>
	</MasterDetailPage.Master>
	<MasterDetailPage.Detail>
		<ContentPage>
			<ContentPage.Content>
			</ContentPage.Content>
		</ContentPage>
	</MasterDetailPage.Detail>
</MasterDetailPage>